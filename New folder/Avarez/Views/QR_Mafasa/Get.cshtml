    .LayoutConfig(new HBoxLayoutConfig { Align=HBoxAlign.Middle}).Items(
        X.Label("لطفا تا دانلود کامل فایل مربوط به مفاصا حساب منتظر بمانید. با تشکر")
        
        .Cls("reddd"))

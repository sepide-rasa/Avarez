@(

 X.Panel()
    .Border(false).Listeners(l=>l.AfterRender.Handler="downloadMafasaaa()")
    .ID("get")
    .AutoScroll(false)
    .CloseAction(Ext.Net.CloseAction.Destroy)
    .Layout(LayoutType.VBox)
    .LayoutConfig(new VBoxLayoutConfig { Align=VBoxAlign.Center})
    .Items
    (
        X.Panel()
            .Border(false)
            .ItemID("pnlget")
            .Listeners(l=>l.Render.Handler="App.get.queryById('pnlget').setHeight(Ext.getBody().getViewSize().height-200);")

                UserName: $("#txtUserName").val(),
                Password: $("#txtPassword").val(),
                Captcha: $("#Captcha2").val()
            };
            $.ajax({
                type: "POST",
                url: url,
                data: data,
                datatype: "json",
                success: function (result) {
                        if (result.Err != "") {
                            $("#lblError").html(result.Err);
                            if (result.HaveCaptcha2 > 1) {
                                $("#capt2").show();
                                $("#_capt2").show();
                                $("#m_imgCaptcha2").show();
                            }
                        }
                        else {
                            $("#lblError").html("");


@using Ext.Net;
@using Ext.Net.MVC;
@using Ext.Net.Utilities;
@{
    var X = Html.X();
}
@(
 Html.X().Panel().Height(560)
     .Listeners(l => l.AfterRender.Handler = "SetSizeJoziyatAvarez()")
        //.Title("پیش نمایش")
                .ID("PrintJoziyatAvarez")
        .AutoScroll(true)
        .Closable(false)
        .CloseAction(Ext.Net.CloseAction.Destroy)
                           .Html("<center><object class='se-pre-con' id='PrintTabJoziyatAvarez' style='width: 1000px;height: 600px;border: 1px solid #ccc;' type='application/pdf' data='/NewVer/SelectParvande/RptJoziyatAvarez?carId=" + @ViewBag.Id + "'></object></center>")
)


<script type="text/javascript">
    function SetSizeJoziyatAvarez() {
        App.PrintJoziyatAvarez.setHeight(Ext.getBody().getViewSize().height);
        $('#PrintTabJoziyatAvarez').height(Ext.getBody().getViewSize().height - 10);
    }

</script>


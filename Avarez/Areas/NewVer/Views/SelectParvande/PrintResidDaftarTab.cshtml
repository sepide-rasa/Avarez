@using Ext.Net;
@using Ext.Net.MVC;
@using Ext.Net.Utilities;
@{
    var X = Html.X();
}
@(
 Html.X().Panel().Height(560)
     .Listeners(l => l.AfterRender.Handler = "SetSizeResidDaftar()")
        //.Title("پیش نمایش")
                .ID("PrintResidDaftar")
        .AutoScroll(true)
        .Closable(false)
        .CloseAction(Ext.Net.CloseAction.Destroy)
                .Html("<center><object class='se-pre-con' id='PrintTabResidDaftar' style='width: 1000px;height: 600px;border: 1px solid #ccc;' type='application/pdf' data='/NewVer/SelectParvande/OfficeRecipt?id=" + @ViewBag.Id + "'></object></center>")
)


<script type="text/javascript">
    function SetSizeResidDaftar() {
        App.PrintResidDaftar.setHeight(Ext.getBody().getViewSize().height);
        $('#PrintTabResidDaftar').height(Ext.getBody().getViewSize().height - 10);
    }

</script>



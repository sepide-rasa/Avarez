<div class="modal" id="YesNoMsg">
    <script src="@Url.Content("~/Content/Base.js")" type="text/javascript"></script>
    @Avarez.Helper.winClass.windowHeader("", "هشدار", 250, 180)
    <br />
    <div id="message">
    </div>
    <br />
    @Avarez.Helper.winClass.Buttons("yes", "بله","yes")
    @Avarez.Helper.winClass.Buttons("No", "خیر","no")
    <br />
    <script type="text/javascript">
        $("document").ready(function () {
            $("#yes").button();
            $("#No").button();
            $("#yes").click(function () {
                var id = '@ViewBag.id';
                var mablagh = '@ViewBag.mablagh';
                var url = '@ViewBag.URL' + '?id=' + id + '&mablagh=' + mablagh;
                //                window.open(url);
                if (isWinOpen == false) {
                    $('#Lock').show();
                    isWinOpen = true;
                    windowAppend('body', '/Home/PreviewRptPDFBox');
                    var t = url;
                    $('#pdf').html("<br/><object style='width: 900px;height: 500px;border: 1px solid #ccc;' id='pdfbox' type='application/pdf' data='" + t + "'></object> ");
                    $('#Lock').hide();
                }
                $("#YesNoMsg").remove();
            });

            $("#No").click(function () {
                //                window.open('/inFacture/FishReport/' + carid);
                if (isWinOpen == false) {
                    isWinOpen = true;
                    windowAppend('body', '/Home/PreviewRptPDFBox');
                    var t = '/inFacture/FishReport/' + carid;
                    $('#pdf').html("<br/><object style='width: 900px;height: 500px;border: 1px solid #ccc;' id='pdfbox' type='application/pdf' data='" + t + "'></object> ");
                }
                $("#YesNoMsg").remove();
            });
        });
    
</script>
@Avarez.Helper.winClass.windowFother()
</div>
